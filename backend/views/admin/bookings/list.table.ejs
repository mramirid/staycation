<!-- DataTales Example -->
<div class="card shadow mb-4">
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>Invoice ID</th>
            <th>Date</th>
            <th>Property Title</th>
            <th>Member Name</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% bookings.forEach((booking) => { %>
          <tr>
            <td><%= booking.invoiceId %></td>
            <td><%= booking.dateRange %></td>
            <td><%= booking.property.current.title %></td>
            <td><%= booking.memberFullName %></td>
            <td><%= booking.payment.status %></td>
            <td>
              <a href="/admin/bookings/<%= booking.id %>" class="btn btn-info btn-circle btn-sm">
                <i class="fas fa-eye"></i>
              </a>
            </td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>